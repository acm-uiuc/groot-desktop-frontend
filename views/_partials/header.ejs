<html>
  <head>
    <meta charset="utf-8"/>
    <title>Association for Computing Machinery - University of Illinois Urbana-Champaign</title>
    <meta name="author" content=""/>
    <meta name="description" content="The Official Website of ACM@UIUC"/>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/foundation/6.2.4/foundation.min.css"/>
    <link rel="stylesheet" type="text/css" href="css/main.css"/>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Roboto" rel="stylesheet">
  </head>
  <body>
    <div class="viewbox">
      <div class="top-bar nav">
        <div class="top-bar-left">
          <ul data-dropdown-menu="data-dropdown-menu" class="dropdown menu">
            <li class="menu-text"><span id="title"><a id="nav-home" href="/"><img src="images/acm-logo-flat.svg" class="logo"/>
                  <h3 id="title-text">ACM@UIUC</h3></a></span></li>
                  <li class="nav-loc">
                    <a href="/about" > About </a>
                  </li>
                  <li class="nav-loc">
                    <a href="/sigs" >SIGs</a>
                  </li>
                  <li class="nav-loc">
                    <a href="/events" >Events</a>
                  </li>
                  <li class="nav-loc">
                    <a href="/conference" >Reflections|Projections</a>
                  </li>
                  <li class="nav-loc">
                    <a href="/sponsors" >Sponsors</a>
                  </li>
                  <% if (!authenticated) { %>
                    <li class="nav-loc">
                      <a href="/join" >Join</a>
                    </li>
                  <% }  else { %>
                    <li class="nav-loc">
                      <a href="/intranet" >Intranet</a>
                    </li>
                  <% } %>
          </ul>
        </div>
        <div class="top-bar-right">
          <div class="top-bar-right">
            <ul class="dropdown menu">
              <% if (!authenticated) { %>
                <li><a id="nav-login" href="/login">
                  <button id="login" class="button">Login</button></a></li>
              <% } else { %>
                <li><a id="nav-login" href="/logout">
                  <button id="logout" class="button">Logout</button></a></li>
              <% } %>
            </ul>
          </div>
        </div>
      </div>
